<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="StartMaui.Modules.Home.HomePage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:StartMaui.Modules.Home"
    BackgroundColor="White"
    Title="{Binding Title}">
    
	<ContentPage.Content>
		<StackLayout>
			<Label Text="Home"
			       HorizontalOptions="CenterAndExpand"
			       TextColor="Purple"
			       VerticalOptions="CenterAndExpand"/>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>